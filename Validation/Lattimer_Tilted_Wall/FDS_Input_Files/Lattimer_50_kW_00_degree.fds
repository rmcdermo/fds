&HEAD CHID=Lattimer_50_kW_00_degree, TITLE='Surface temperature on slopped wall' /

MESH IJK=60,60,60, XB=-.75,.75,-.75,.75,-0.2,1.3 /

&MESH ID='Mesh01', IJK=32,28,30, XB=-1.0,-0.2,-0.35,0.35,-0.2,0.55/
&MESH ID='Mesh02', IJK=32,28,30, XB=-1.0,-0.2,-0.35,0.35,0.55,1.3/
&MESH ID='Mesh03', IJK=32,28,30, XB=-0.2,0.6,-0.35,0.35,-0.2,0.55/
&MESH ID='Mesh04', IJK=32,28,30, XB=-0.2,0.6,-0.35,0.35,0.55,1.3/

&TIME T_END=300./

&DUMP DT_SLCF=0.1, DT_BNDF=1., DT_DEVC=1. /

&DEVC XYZ=0,0,0, ID='burner on', QUANTITY='TIME', SETPOINT=400., INITIAL_STATE=.TRUE. /
&SURF ID='burner', HRRPUA=555.6, TMP_FRONT=400., COLOR='RED' /
&VENT XB=0.00,0.30,-0.15,0.15,0.001,0.001, SURF_ID='burner', DEVC_ID='burner on' /
&OBST XB=0.00,0.30,-0.15,0.15,-0.10,0.00 /

&MOVE ID='SAMPLE-ROTATE', AXIS=0,-1,0, ROTATION_ANGLE=0.0, X0=0.0, Y0=0.0, Z0=0.0, DZ=0., DX=0., DY=0./
&GEOM ID='WALL', SURF_ID='ceramic board', XB=-0.0254,0.0,-0.305,0.305,0.0,0.915, MOVE_ID='SAMPLE-ROTATE', CELL_BLOCK_IOR=-3,  /

&MATL ID='ceramic', CONDUCTIVITY=.15, SPECIFIC_HEAT=1., DENSITY=272. /
&SURF ID='ceramic board', EMISSIVITY=0.95, COLOR='GRAY 20', MATL_ID='ceramic', THICKNESS=0.005, /

&REAC FUEL='PROPANE', SOOT_YIELD=0.01/

&VENT MB='XMIN', SURF_ID='OPEN'/
&VENT MB='XMAX', SURF_ID='OPEN'/
&VENT MB='YMIN', SURF_ID='OPEN'/
&VENT MB='YMAX', SURF_ID='OPEN'/
&VENT MB='ZMAX', SURF_ID='OPEN'/

&BNDF QUANTITY='WALL TEMPERATURE' /
&BNDF QUANTITY='GAS TEMPERATURE' /
&BNDF QUANTITY='GAUGE HEAT FLUX' /

&SLCF PBX=0.1, QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0., QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0., QUANTITY='VELOCITY', VECTOR=.TRUE. /

&DEVC ID='TC01', QUANTITY='GAS TEMPERATURE', XYZ=-0.0,0,0.05, IOR=1/
&DEVC ID='GHF01', QUANTITY='GAUGE HEAT FLUX', XYZ=-0.0,0,0.05, IOR=1/

&DEVC ID='TC02', QUANTITY='GAS TEMPERATURE', XYZ=-0.0,0,0.15, IOR=1/
&DEVC ID='GHF02', QUANTITY='GAUGE HEAT FLUX', XYZ=-0.0,0,0.15, IOR=1/

&DEVC ID='TC03', QUANTITY='GAS TEMPERATURE', XYZ=-0.0,0,0.25, IOR=1/
&DEVC ID='GHF03', QUANTITY='GAUGE HEAT FLUX', XYZ=-0.0,0,0.25, IOR=1/

&DEVC ID='TC04', QUANTITY='GAS TEMPERATURE', XYZ=-0.0,0,0.35, IOR=1/
&DEVC ID='GHF04', QUANTITY='GAUGE HEAT FLUX', XYZ=-0.0,0,0.35, IOR=1/

&DEVC ID='TC05', QUANTITY='GAS TEMPERATURE', XYZ=-0.0,0,0.45, IOR=1/
&DEVC ID='GHF05', QUANTITY='GAUGE HEAT FLUX', XYZ=-0.0,0,0.45, IOR=1/

&DEVC ID='TC06', QUANTITY='GAS TEMPERATURE', XYZ=-0.0,0,0.55, IOR=1/
&DEVC ID='GHF06', QUANTITY='GAUGE HEAT FLUX', XYZ=-0.0,0,0.55, IOR=1/

&DEVC ID='TC07', QUANTITY='GAS TEMPERATURE', XYZ=-0.0,0,0.65, IOR=1/
&DEVC ID='GHF07', QUANTITY='GAUGE HEAT FLUX', XYZ=-0.0,0,0.65, IOR=1/

&DEVC ID='TC08', QUANTITY='GAS TEMPERATURE', XYZ=-0.0,0,0.75, IOR=1/
&DEVC ID='GHF08', QUANTITY='GAUGE HEAT FLUX', XYZ=-0.0,0,0.75, IOR=1/

&DEVC ID='TC09', QUANTITY='GAS TEMPERATURE', XYZ=-0.0,0,0.85, IOR=1/
