&HEAD CHID='Lattimer_20_kW_20_degree_ibm', TITLE='Surface temperature on slopped wall, 20 kW propane fire' /

&MESH IJK=60,60,60, XB=-.75,.75,-.75,.75,-0.2,1.3 /

&TIME T_END=500./

&DUMP DT_SLCF=0.1, DT_BNDF=1., DT_DEVC=1. /

&DEVC XYZ=0,0,0, ID='burner on', QUANTITY='TIME', SETPOINT=400., INITIAL_STATE=.TRUE. /
&SURF ID='burner', HRRPUA=222., TMP_FRONT=400., COLOR='RED' /
&VENT XB=0.00,0.30,-0.15,0.15,0.001,0.001, SURF_ID='burner', DEVC_ID='burner on' /
&OBST XB=0.00,0.30,-0.15,0.15,-0.10,0.00 /

&MOVE ID='SAMPLE-ROTATE', AXIS=0,-1,0, ROTATION_ANGLE=20, X0=0.0, Y0=0.0, Z0=0.0, DZ=0., DX=0., DY=0./
&GEOM ID='WALL', SURF_ID='ceramic board', XB=-0.0254,0.0,-0.305,0.305,0.0,0.915, MOVE_ID='SAMPLE-ROTATE',  /

&MATL ID='ceramic', CONDUCTIVITY=.15, SPECIFIC_HEAT=1., DENSITY=272. /
&SURF ID='ceramic board', EMISSIVITY=0.95, COLOR='GRAY 20', MATL_ID='ceramic', RADIUS=0.005, GEOMETRY='SPHERICAL' /

&REAC FUEL='PROPANE', SOOT_YIELD=0.01/

&VENT MB='XMIN', SURF_ID='OPEN'/
&VENT MB='XMAX', SURF_ID='OPEN'/
&VENT MB='YMIN', SURF_ID='OPEN'/
&VENT MB='YMAX', SURF_ID='OPEN'/
&VENT MB='ZMAX', SURF_ID='OPEN'/

&BNDF QUANTITY='WALL TEMPERATURE' /
&BNDF QUANTITY='GAUGE HEAT FLUX' /

&SLCF PBX=0.1, QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0., QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=0., QUANTITY='VELOCITY', VECTOR=.TRUE. /

&DEVC QUANTITY='WALL TEMPERATURE', ID='TC1', XYZ=-0.0257,0,0.0705, IOR=1 /
&DEVC QUANTITY='WALL TEMPERATURE', ID='TC2', XYZ=-0.1043,0,0.2866, IOR=1 /
