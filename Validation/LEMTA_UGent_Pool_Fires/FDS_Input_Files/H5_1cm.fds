&HEAD CHID='H5_1cm', TITLE='Heptane pool fire, 1 cm grid' /

&MISC TMPA=20 /

&MESH IJK=16,16,16, XB=-0.16, 0.00,-0.16, 0.00, 0.00, 0.16, MULT_ID='mesh' /
&MULT ID='mesh', DX=0.16, DY=0.16, DZ=0.16, I_UPPER=1, J_UPPER=1, K_UPPER=3 /

&TIME T_END=2000 / 

CO and soot yields from https://doi.org/10.1016/j.firesaf.2016.01.002
radiative fraction from https://doi.org/10.1016/j.firesaf.2023.104044 

&REAC FUEL='N-HEPTANE', CO_YIELD=0.010, SOOT_YIELD=0.037, RADIATIVE_FRACTION=0.29 /

heat of reaction, specific heat, density and boiling temperature from https://doi.org/10.1016/j.firesaf.2016.01.002
actual conductivity of 0.14 (from https://doi.org/10.1016/j.firesaf.2016.01.002) is multiplied by 12 (https://doi.org/10.1016/j.firesaf.2023.104044) 

&MATL ID = 'HEPTANE LIQUID'
      EMISSIVITY = 1.
      NU_SPEC = 1.
      SPEC_ID = 'N-HEPTANE'
      HEAT_OF_REACTION = 317.
      CONDUCTIVITY = 1.68  
      SPECIFIC_HEAT = 2.24
      DENSITY = 675.
      BOILING_TEMPERATURE = 98.5 / 

properties of (mild) steel from the book of D. Drysdale

&MATL ID = 'STEEL'
      CONDUCTIVITY = 45.8  
      SPECIFIC_HEAT = 0.46
      DENSITY = 7850. /

https://www.fermacell.co.uk/en

&MATL ID = 'FERMACELL'
      CONDUCTIVITY = 0.32  
      SPECIFIC_HEAT = 1.10
      DENSITY = 1150. /

&SURF ID = 'STEEL SHEET'
      MATL_ID = 'STEEL'
      THICKNESS = 0.002
      COLOR = 'BLACK' /

&SURF ID = 'HEPTANE POOL'
      COLOR = 'ORANGE'
      MATL_ID = 'HEPTANE LIQUID','STEEL','FERMACELL'
      THICKNESS = 0.046, 0.002, 0.010
      CELL_SIZE = 0.001
      MASS_TRANSFER_COEFFICIENT = 0.0015
      CONVECTION_LENGTH_SCALE = 0.05 /
    
&OBST XB=-0.02, 0.03,-0.02, 0.03, 0.00, 0.04, SURF_IDS='HEPTANE POOL','STEEL SHEET','STEEL SHEET'/ 

&OBST XB=-0.02,-0.02,-0.02, 0.03, 0.00, 0.05, SURF_ID='STEEL SHEET'/ 
&OBST XB= 0.03, 0.03,-0.02, 0.03, 0.00, 0.05, SURF_ID='STEEL SHEET'/ 
&OBST XB=-0.02, 0.03, 0.03, 0.03, 0.00, 0.05, SURF_ID='STEEL SHEET'/ 
&OBST XB=-0.02, 0.03,-0.02,-0.02, 0.00, 0.05, SURF_ID='STEEL SHEET'/

&VENT MB='YMAX', SURF_ID='OPEN' / 
&VENT MB='YMIN', SURF_ID='OPEN' / 
&VENT MB='XMAX', SURF_ID='OPEN' / 
&VENT MB='XMIN', SURF_ID='OPEN' / 
&VENT MB='ZMIN', SURF_ID='OPEN' /
&VENT MB='ZMAX', SURF_ID='OPEN' /

&DUMP DT_DEVC=10., DT_HRR=30. /

&SLCF PBY=0.0,QUANTITY='TEMPERATURE'/
&SLCF PBY=0.0,QUANTITY='VELOCITY',   VECTOR=.TRUE./
&SLCF PBY=0.0,QUANTITY='U-VELOCITY', VECTOR=.TRUE./
&SLCF PBY=0.0,QUANTITY='V-VELOCITY', VECTOR=.TRUE./
&SLCF PBY=0.0,QUANTITY='W-VELOCITY', VECTOR=.TRUE./

&DEVC XYZ=0.000, 0.000, 0.040, QUANTITY='WALL THICKNESS', IOR=3, ID='LIQUID HEIGHT', CONVERSION_ADDEND=-0.012 /

&DEVC XYZ=0.000, 0.000, 0.040, QUANTITY='WALL TEMPERATURE', ID='SURFACE TEMPERATURE', IOR=3 /

&DEVC XYZ=0.000, 0.000, 0.040, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=-0.017, ID='T05', IOR=3 /
&DEVC XYZ=0.000, 0.000, 0.040, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=-0.022, ID='T10', IOR=3 /
&DEVC XYZ=0.000, 0.000, 0.040, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=-0.027, ID='T15', IOR=3 /
&DEVC XYZ=0.000, 0.000, 0.040, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=-0.032, ID='T20', IOR=3 /
&DEVC XYZ=0.000, 0.000, 0.040, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=-0.037, ID='T25', IOR=3 /
&DEVC XYZ=0.000, 0.000, 0.040, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=-0.042, ID='T30', IOR=3 /
&DEVC XYZ=0.000, 0.000, 0.040, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=-0.047, ID='T35', IOR=3 /
&DEVC XYZ=0.000, 0.000, 0.040, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=-0.052, ID='T40', IOR=3 /
&DEVC XYZ=0.000, 0.000, 0.040, QUANTITY='INSIDE WALL TEMPERATURE', DEPTH=-0.057, ID='T45', IOR=3 /

&DEVC XB=-0.07, 0.08,-0.07, 0.08, 0.04, 0.04, QUANTITY='RADIATIVE HEAT FLUX',  IOR=3, SPATIAL_STATISTIC='SURFACE INTEGRAL', ID='RHF'/
&DEVC XB=-0.07, 0.08,-0.07, 0.08, 0.04, 0.04, QUANTITY='CONVECTIVE HEAT FLUX', IOR=3, SPATIAL_STATISTIC='SURFACE INTEGRAL', ID='CHF'/
&DEVC XB=-0.07, 0.08,-0.07, 0.08, 0.04, 0.04, QUANTITY='TOTAL HEAT FLUX',      IOR=3, SPATIAL_STATISTIC='SURFACE INTEGRAL', ID='THF'/
&DEVC XB=-0.07, 0.08,-0.07, 0.08, 0.04, 0.04, QUANTITY='NET HEAT FLUX',        IOR=3, SPATIAL_STATISTIC='SURFACE INTEGRAL', ID='NHF'/

&DEVC XB=-0.07, 0.08,-0.07, 0.08, 0.04, 0.04, QUANTITY='BURNING RATE',         IOR=3, SPATIAL_STATISTIC='MEAN', ID='MLRPUA', CONVERSION_FACTOR=1000. /

&BNDF QUANTITY='WALL TEMPERATURE', CELL_CENTERED=T /
&BNDF QUANTITY='GAUGE HEAT FLUX', CELL_CENTERED=T /
&BNDF QUANTITY='RADIATIVE HEAT FLUX', CELL_CENTERED=T /
&BNDF QUANTITY='CONVECTIVE HEAT FLUX', CELL_CENTERED=T /
&BNDF QUANTITY='HEAT TRANSFER COEFFICIENT', CELL_CENTERED=T /
&BNDF QUANTITY='BURNING RATE', CELL_CENTERED=T /

&TAIL /


