&HEAD CHID='Sandia_He_1m_dx20cm', TITLE='Sandia 1m helium plume, 20 cm resolution' /

! Reference: T. J. O'Hern, E. J. Weckman, A. L. Gerhart, S. R. Tieszen, and R. W. Schefer,
! Experimental study of a turbulent buoyant helium plume, J. Fluid Mech., 544:143-171, 2005.

! &MULT ID='mesh array', DX=1.5,DY=1.5,DZ=1.0, I_UPPER=1,J_UPPER=1,K_UPPER=3 /
! &MESH IJK=15,15,10, XB=-1.5,0.0,-1.5,0.0,0,1, MULT_ID='mesh array' /

! &MULT ID='outer mesh array', DX=1.5,DY=1.5,DZ=1.0,
!       I_LOWER=0,I_LOWER_SKIP=1,I_UPPER_SKIP=2,I_UPPER=3,
!       J_LOWER=0,J_LOWER_SKIP=1,J_UPPER_SKIP=2,J_UPPER=3,
!       K_UPPER=3 /
! &MESH IJK=15,15,10, XB=-3.0,-1.5,-3.0,-1.5,0,1, MULT_ID='outer mesh array' /

&MESH IJK=30,30,20, XB=-3,3,-3,3,0,4/

&TIME T_END=40. /

&DUMP SIG_FIGS=4, SIG_FIGS_EXP=2 /

&MISC TMPA=12.
      P_INF=80900.
      HUMIDITY=20.
      SIMULATION_MODE='LES'/

&SPEC ID='HELIUM'/
&SPEC ID='ACETONE'/
&SPEC ID='OXYGEN'/

&RADI RADIATION=.FALSE./

&MATL ID='STEEL', CONDUCTIVITY=54., SPECIFIC_HEAT=0.465, DENSITY=7850., EMISSIVITY=0.9 /
&SURF ID='PLATE', COLOR='GRAY', DEFAULT=T, MATL_ID='STEEL', THICKNESS=0.025 /

! plume source is 0.325 m/s * pi*(.5)^2 = 0.255254 m3/s
! composition is HELIUM  0.964 vol frac (0.7075 mass frac)
!                ACETONE 0.017 vol frac (0.1809 mass frac)
!                OXYGEN  0.019 vol frac (0.1116 mass frac)

&SURF ID='PLUME', SPEC_ID(1)='HELIUM',  MASS_FRACTION(1)=0.7075,
                  SPEC_ID(2)='ACETONE', MASS_FRACTION(2)=0.1809,
                  SPEC_ID(3)='OXYGEN',  MASS_FRACTION(3)=0.1116,
                  VEL=-0.325, TMP_FRONT=12. /

&VENT XB=-0.5,0.5,-0.5,0.5,0,0, XYZ=0,0,0.0001, RADIUS=0.5, COLOR='BLUE', SURF_ID='PLUME', SPREAD_RATE=1000 /

&VENT XB=-1,1,-1,1,0,0, XYZ=0,0,0, RADIUS=1, SURF_ID='PLATE'/

&SURF ID='DEFAULT WALL', DEFAULT=T, COLOR='BEIGE', TRANSPARENCY=0.5/
&VENT MB='ZMIN',SURF_ID='OPEN'/
&VENT XB=-1.15,1.15,-1.15,1.15,3.999,3.999,SURF_ID='OPEN'/

&SLCF PBY=-.0625, QUANTITY='VELOCITY', VECTOR=T /
&SLCF PBY=-.0625, QUANTITY='DENSITY', CELL_CENTERED=T /
&SLCF PBY=-.0625, QUANTITY='MASS FRACTION', SPEC_ID='HELIUM', CELL_CENTERED=T /
&SLCF PBY=-.0625, QUANTITY='MASS FRACTION', SPEC_ID='ACETONE', CELL_CENTERED=T /

Radial profiles of velocity and mass fraction

! CONVERSION_FACTOR = 1/Y_ACETONE = 1/0.1809 = 5.5279 converts ACETONE mass fraction to PLUME FLUID mass fraction

&DEVC XB=-0.46875,0.46875,0.0,0.0,0.2,0.2, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE AVERAGE', QUANTITY='CELL W', ID='Wp2', STATISTICS_START=10., POINTS=16, X_ID='x' /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE AVERAGE', QUANTITY='CELL W', ID='Wp4', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE AVERAGE', QUANTITY='CELL W', ID='Wp6', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /

&DEVC XB=-0.46875,0.46875,0.0,0.0,0.2,0.2, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE AVERAGE', QUANTITY='CELL U', ID='Up2', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE AVERAGE', QUANTITY='CELL U', ID='Up4', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE AVERAGE', QUANTITY='CELL U', ID='Up6', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /

&DEVC XB=-0.46875,0.46875,0.0,0.0,0.2,0.2, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE AVERAGE', QUANTITY='MASS FRACTION', SPEC_ID='HELIUM', ID='YHEp2', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE AVERAGE', QUANTITY='MASS FRACTION', SPEC_ID='HELIUM', ID='YHEp4', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE AVERAGE', QUANTITY='MASS FRACTION', SPEC_ID='HELIUM', ID='YHEp6', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /

&DEVC XB=-0.46875,0.46875,0.0,0.0,0.2,0.2, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE AVERAGE', QUANTITY='MASS FRACTION', SPEC_ID='ACETONE', CONVERSION_FACTOR=5.5279, ID='YPFp2', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE AVERAGE', QUANTITY='MASS FRACTION', SPEC_ID='ACETONE', CONVERSION_FACTOR=5.5279, ID='YPFp4', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE AVERAGE', QUANTITY='MASS FRACTION', SPEC_ID='ACETONE', CONVERSION_FACTOR=5.5279, ID='YPFp6', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /

&DEVC XB=-0.46875,0.46875,0.0,0.0,0.2,0.2, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE RMS', QUANTITY='CELL W', ID='Wp2_rms', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE RMS', QUANTITY='CELL W', ID='Wp4_rms', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE RMS', QUANTITY='CELL W', ID='Wp6_rms', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /

&DEVC XB=-0.46875,0.46875,0.0,0.0,0.2,0.2, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE RMS', QUANTITY='CELL U', ID='Up2_rms', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE RMS', QUANTITY='CELL U', ID='Up4_rms', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE RMS', QUANTITY='CELL U', ID='Up6_rms', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /

&DEVC XB=-0.46875,0.46875,0.0,0.0,0.2,0.2, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE RMS', QUANTITY='MASS FRACTION', SPEC_ID='HELIUM', ID='YHEp2_rms', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE RMS', QUANTITY='MASS FRACTION', SPEC_ID='HELIUM', ID='YHEp4_rms', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE RMS', QUANTITY='MASS FRACTION', SPEC_ID='HELIUM', ID='YHEp6_rms', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /

&DEVC XB=-0.46875,0.46875,0.0,0.0,0.2,0.2, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE RMS', QUANTITY='MASS FRACTION', SPEC_ID='ACETONE', CONVERSION_FACTOR=5.5279, ID='YPFp2_rms', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.4,0.4, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE RMS', QUANTITY='MASS FRACTION', SPEC_ID='ACETONE', CONVERSION_FACTOR=5.5279, ID='YPFp4_rms', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /
&DEVC XB=-0.46875,0.46875,0.0,0.0,0.6,0.6, SPATIAL_STATISTIC='INTERPOLATION', TEMPORAL_STATISTIC='FAVRE RMS', QUANTITY='MASS FRACTION', SPEC_ID='ACETONE', CONVERSION_FACTOR=5.5279, ID='YPFp6_rms', STATISTICS_START=10., POINTS=16, HIDE_COORDINATES=T /

&TAIL /
