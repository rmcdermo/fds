! J. Kim and P. Moin. Transport of Passive Scalars in Turbulent Channel Flow. NASA Technical Memorandum 89463, 1987.

&HEAD CHID='heated_channel_Pr_0p71_8', TITLE='LES of turbulent channel with heat source with wall model, Pr=0.71' /

MESH IJK=64,32,32, XB=0,12,0,6,0,2/ original mesh setup by Jung-Il Choi

&MULT DX=2,DY=2,DZ=2, I_UPPER=5,J_UPPER=2, ID='m1'/
&MESH IJK=8,8,8, XB=0,2,0,2,0,2, MULT_ID='m1'/

&INIT XB=0,12,0,6,0,2, HRRPUV=0.015034614 /

&TIME T_END=30000.0/
&DUMP DT_SLCF=100.0, DT_DEVC=5000.0 /

! see Kim & Moin Fig 4 for turbulent Prandtl number for each Pr
&MISC SIMULATION_MODE='LES', TENSOR_DIFFUSIVITY=T, CHECK_HT=T, STRATIFICATION=F, CONSTANT_SPECIFIC_HEAT_RATIO=T, GVEC(3)=0., PR=1.00, TURBULENCE_MODEL='WALE'/

&WIND U0=0.045, FORCE_VECTOR(1)=9.0088e-06 / FORCE_VECTOR = dpdx based on Re_tau=180

&PRES SOLVER='FFT', MAX_PRESSURE_ITERATIONS=10, CHECK_POISSON=T/

&RADI RADIATION=.FALSE./

&SPEC ID='LJ AIR', SPECIFIC_HEAT=1., CONDUCTIVITY=0.025656, VISCOSITY=1.8216E-5, BACKGROUND=.TRUE./ ! Pr=0.71

&VENT PBX=0,  SURF_ID='PERIODIC' /
&VENT PBX=12, SURF_ID='PERIODIC' /
&VENT PBY=0,  SURF_ID='PERIODIC' /
&VENT PBY=6,  SURF_ID='PERIODIC' /

&VENT DB='ZMIN', SURF_ID='U_WALL' /
&VENT DB='ZMAX', SURF_ID='D_WALL' /

&OBST XB=0.0625,0.125,0.,6.,1.9375,2.0, DEVC_ID='trip', COLOR='RED', THICKEN=T /
&OBST XB=0.0625,0.125,0.,6.,0.0,0.0625, DEVC_ID='trip', COLOR='RED', THICKEN=T /
&DEVC XYZ=6,3,1, ID='trip', QUANTITY='TIME', SETPOINT=5000., INITIAL_STATE=.TRUE. /

&SURF ID = 'U_WALL', COLOR = 'BLUE', TMP_FRONT = 20., HEAT_TRANSFER_MODEL = 'RAYLEIGH'/
&SURF ID = 'D_WALL', COLOR = 'BLUE', TMP_FRONT = 20., HEAT_TRANSFER_MODEL = 'RAYLEIGH'/

&BNDF QUANTITY='VISCOUS WALL UNITS' /
&BNDF QUANTITY='THERMAL WALL UNITS' /
&BNDF QUANTITY='HEAT TRANSFER COEFFICIENT' /

&SLCF PBY=3, QUANTITY='VELOCITY', VECTOR=.TRUE. /
&SLCF PBY=3, QUANTITY='TEMPERATURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=3, QUANTITY='VISCOSITY', CELL_CENTERED=.TRUE. /
&SLCF PBY=3, QUANTITY='BACKGROUND PRESSURE', CELL_CENTERED=.TRUE. /
&SLCF PBY=3, QUANTITY='HRRPUV', CELL_CENTERED=.TRUE. /

&DEVC XB=1,1,0,6,0,2, SPATIAL_STATISTIC='MEAN',QUANTITY='DENSITY', ID='RHO'/
&DEVC XB=1,1,0,6,0,2, SPATIAL_STATISTIC='MEAN',QUANTITY='U-VELOCITY', ID='U-VEL'/

&DEVC XB=0.0,2,0.0,2,0.00000,0.00000, SPATIAL_STATISTIC='MEAN',QUANTITY='CONVECTIVE HEAT FLUX', ID='HF0B'/
