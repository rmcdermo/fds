&HEAD CHID='NIST_TGA_10K', TITLE='5 mg of Black PMMA heated at 10 K/min' /

&MESH IJK=3,3,3, XB=-0.015,0.015,-0.015,0.015,-0.015,0.015 /

&TIME T_END=4200., WALL_INCREMENT=1, DT=0.1 /

&MISC TMPA=27. /

&RAMP ID='T-ramp', T=   0., F=0. /
&RAMP ID='T-ramp', T=4200., F=1. /

&SPEC ID='NITROGEN', BACKGROUND=T /
&SPEC ID='MMA', FORMULA='C5O2H8' /

&RADI RADIATION=F /

&MATL ID='BLACK PMMA'
      REFERENCE_TEMPERATURE=370.
      REFERENCE_RATE=0.0034
      HEATING_RATE=10
      DENSITY=1100.
      SPEC_ID='MMA'
      NU_SPEC=1.
      HEAT_OF_REACTION=870.
      HEAT_OF_COMBUSTION=24000.
      CONDUCTIVITY = 0.20
      SPECIFIC_HEAT = 2.2 /

&MATL ID='INSULATION'
      DENSITY=10.
      CONDUCTIVITY = 0.01
      SPECIFIC_HEAT = 1.0 /

&SURF ID='PMMA BC'
      COLOR='BLACK'
      GEOMETRY='SPHERICAL'
      TMP_GAS_FRONT=727., RAMP_TMP_GAS_FRONT='T-ramp'
      HEAT_TRANSFER_COEFFICIENT=500.
      N_LAYER_CELLS_MAX=1
      MATL_ID='BLACK PMMA','INSULATION'
      THICKNESS=0.0010178,0.00001 /

&PART ID='BALL', SURF_ID='PMMA BC', STATIC=T, QUANTITIES(1:2)='PARTICLE TEMPERATURE','PARTICLE MASS' /

&INIT ID='BALL', XYZ=0,0,0, N_PARTICLES=1, PART_ID='BALL' /

&DUMP DT_DEVC=1. /

&DEVC ID='Temperature', INIT_ID='BALL', QUANTITY='WALL TEMPERATURE', CONVERSION_ADDEND=273.15, UNITS='[K]' /
&DEVC ID='Mass', XB=-0.015,0.015,-0.015,0.015,-0.015,0.0150, QUANTITY='MPUV', PART_ID='BALL', STATISTICS='VOLUME INTEGRAL'
      CONVERSION_FACTOR=1E6, UNITS='[mg]' /

&TAIL / 


