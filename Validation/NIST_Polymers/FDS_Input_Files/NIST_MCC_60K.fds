&HEAD CHID='NIST_MCC_60K', TITLE='5 mg of Black PMMA heated at 60 K/min' /

&MESH IJK=3,3,3, XB=-0.015,0.015,-0.015,0.015,0.000,0.030 /

&TIME T_END=700., WALL_INCREMENT=1, DT=0.1 /

&MISC TMPA=27., Y_O2_INFTY=0.001 /

&REAC FUEL='MMA', FORMULA='C5O2H8' /

&RADI RADIATION=F /

&MATL ID='BLACK PMMA'
      REFERENCE_TEMPERATURE=370.
      REFERENCE_RATE=0.0034
      HEATING_RATE=10
      DENSITY=1210.
      SPEC_ID='MMA'
      NU_SPEC=1.
      HEAT_OF_REACTION=817.
      HEAT_OF_COMBUSTION=24000.
      CONDUCTIVITY = 0.20
      SPECIFIC_HEAT = 2.2 /

&SURF ID='PMMA BC'
      COLOR='BLACK'
      TMP_GAS_FRONT=727., RAMP_TMP_GAS_FRONT='T-ramp'
      HEAT_TRANSFER_COEFFICIENT=500.
      N_LAYER_CELLS_MAX=1
      MATL_ID='BLACK PMMA'
      BACKING='INSULATED'
      THICKNESS=0.0001 /

&RAMP ID='T-ramp', T=   0., F=0. /
&RAMP ID='T-ramp', T= 700., F=1. /

&VENT XB=-0.005,0.005,-0.005,0.005,0.000,0.000, SURF_ID='PMMA BC' /

&DUMP DT_DEVC=1. /

&DEVC ID='Temperature', XYZ=0,0,0, IOR=3, QUANTITY='WALL TEMPERATURE', CONVERSION_ADDEND=273.15, UNITS='[K]' /
&DEVC ID='HRR', XYZ=0,0,0, IOR=3, QUANTITY='NORMALIZED HEAT RELEASE RATE', UNITS='[W/g]' /

&TAIL / 


