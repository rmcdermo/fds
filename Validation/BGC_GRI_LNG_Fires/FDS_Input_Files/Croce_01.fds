&DEVC ID='P11B',XYZ=  0.00, 26.91,1.5, QUANTITY='RADIATIVE HEAT FLUX GAS', TEMPORAL_STATISTIC='RUNNING AVERAGE', STATISTICS_START=20., ORIENTATION=0,-1,0 /
