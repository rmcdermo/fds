&HEAD CHID='Croce_01', TITLE='Croce LNG Fires'/

&MESH IJK=50,50,50, XB=-5.,5.,-5.,5.,0.0,10., MULT_ID='mesh1' /
&MULT ID='mesh1', DX=10, DY=10, I_LOWER=-1, I_UPPER=1, J_LOWER=-3, J_UPPER=3 / 21 meshes

&MESH IJK=50,50,50, XB=-5.,5.,-5.,5.,0.0,10., MULT_ID='mesh4' /
