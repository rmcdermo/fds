&HEAD CHID='Cup_methane_SBC_1S',TITLE='Methane Cup Burner, NaHCO3 at critical mass loading, 1 step decomposition'/

! &MESH XB=0,0.04,0.0,0.001,0.05,0.200,IJK=40,1,150,CYLINDRICAL=.TRUE./

&MULT ID='m1',DX=0.01,DZ=0.01,I_UPPER=3,K_UPPER=14/ 60 mesh
&MESH XB=0,0.01,0.0,0.001,0.05,0.06,IJK=10,1,10,CYLINDRICAL=T,MULT_ID='m1'/

&TIME T_END=1, WALL_INCREMENT=1/

&MISC SIMULATION_MODE='DNS',HUMIDITY=0.0,Y_CO2_INFTY=0.0,CHECK_HT=T
      PARTICLE_CFL=T,PARTICLE_CFL_MIN=0.4,PARTICLE_CFL_MAX=0.5/

&CLIP MINIMUM_DENSITY=0.01/

&PRES SOLVER='UGLMAT', MAX_PRESSURE_ITERATIONS=1/

&COMB ZZ_MIN_GLOBAL=1E-15,INITIAL_UNMIXED_FRACTION=0,FINITE_RATE_MIN_TEMP=100.
      ODE_SOLVER='RK2 RICHARDSON',RICHARDSON_ERROR_TOLERANCE=1.E-9,
      MAX_CHEMISTRY_SUBSTEPS=100/

&CATF OTHER_FILES='../Mech/CH4_skeletal_smooke_NaOH.fds'/

&INIT PART_ID='ignitor', XYZ=0.0105,0.0005,0.1035, N_PARTICLES=1 /
&PART ID='ignitor', SAMPLING_FACTOR=1, SURF_ID='hot', STATIC=.TRUE., QUANTITIES='PARTICLE TEMPERATURE' /
&SURF ID='hot', TMP_FRONT=2000., EMISSIVITY=1., RADIUS=0.0005, GEOMETRY='SPHERICAL', RAMP_T='ignitor ramp' /

&RAMP ID='ignitor ramp', T=0.00,F=0/
&RAMP ID='ignitor ramp', T=0.05,F=1/
&RAMP ID='ignitor ramp', T=0.20,F=1/
&RAMP ID='ignitor ramp', T=0.21,F=0/
&RAMP ID='ignitor ramp', T=1.00,F=0/

&MATL ID='PYREX',SPECIFIC_HEAT=0.753, CONDUCTIVITY=1.,DENSITY=2230./
&SURF ID='PYREX',DEFAULT=T,MATL_ID='PYREX',THICKNESS=0.002,COLOR='GRAY 40'/

!&SURF ID='INFLOW',VEL=-0.12,SPEC_ID='OXYGEN',MASS_FRACTION=0.23,TAU_V=0.01,TAU_MF=0.01/
!&SURF ID='INFLOW',
!      MASS_FLUX(1)=0.0331, SPEC_ID(1)='OXYGEN',
!      MASS_FLUX(2)=0.1109, SPEC_ID(2)='NITROGEN',
!      MASS_FLUX(3)=0.0030, SPEC_ID(3)='SODIUM HYDROXIDE',
!      MASS_FLUX(4)=0.0033, SPEC_ID(4)='CARBON DIOXIDE'
!      TAU_MF(1:4)=0,0,0,0 /

&SURF ID='INFLOW',
      COLOR='GREEN'
      VEL=-0.12,TAU_V=0.01
      SPEC_ID='OXYGEN',MASS_FRACTION=0.23,TAU_MF=0.01
      PART_ID='NaHCO3',
      NPPC=1,
      DT_INSERT=1.E-6
      PARTICLE_MASS_FLUX=0.0063,
      RAMP_PART='RAMP PMF'/ mdot"_p = mdot"_a/(100/MEC - 1) where MEC=4.2 mass % for NaHCO3 with propane fuel

&RAMP ID='RAMP PMF',T=0,F=0/
&RAMP ID='RAMP PMF',T=1,F=2/

&SURF ID='NaHCO3'
      MATL_ID = 'Sodium Bicarbonate'
      RADIUS=2e-06
      GEOMETRY='SPHERICAL'
      STRETCH_FACTOR = 1.
      MINIMUM_LAYER_THICKNESS = 1e-10/

&PART ID='NaHCO3',
      SURF_ID='NaHCO3',
      DRAG_LAW ='SPHERE',
      TURBULENT_DISPERSION=T,
      COLOR='GRAY 90',
      CHECK_DISTRIBUTION =.TRUE.,
      MINIMUM_DIAMETER=0.001,
      MONODISPERSE=T,
      SAMPLING_FACTOR=1000,
      QUANTITIES(1)='PARTICLE TEMPERATURE'/

&CATF OTHER_FILES='SBC_MATL_MODEL_1S.fds'/

&SURF ID='BURNER',VEL=-0.01,SPEC_ID(1)='METHANE',MASS_FRACTION(1)=1.,TAU_MF(1)=0./

&OBST XB=0.000,0.010,0.000,0.001,0.085,0.100,SURF_IDS='BURNER','PYREX','PYREX'/
&OBST XB=0.010,0.011,0.000,0.001,0.085,0.103,SURF_ID='PYREX'/
&VENT PBZ=0.05,SURF_ID='INFLOW'/
&VENT PBZ=0.2,SURF_ID='OPEN'/

&SLCF DB='YMID', VECTOR=T, QUANTITY='VELOCITY'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='NITROGEN'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='METHOXY RADICAL'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='HYDROGEN ATOM'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='OXYGEN ATOM'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='METHYL RADICAL'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='FORMYL RADICAL'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='FORMALDEHYDE'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='HYDROPEROXY RADICAL'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='HYDROGEN PEROXIDE'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='HYDROXYL RADICAL'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='CARBON MONOXIDE'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='CARBON DIOXIDE'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='OXYGEN'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='WATER VAPOR'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='METHANE'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='HYDROGEN'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='SODIUM HYDROXIDE'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='SODIUM'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='VOLUME FRACTION',SPEC_ID='SODIUM OXIDE'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='CHEMISTRY SUBITERATIONS'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='TEMPERATURE'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='ENTHALPY'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='STRAIN RATE'/
&SLCF DB='YMID', CELL_CENTERED=T, QUANTITY='MPUV',PART_ID='NaHCO3'/


&TAIL /
