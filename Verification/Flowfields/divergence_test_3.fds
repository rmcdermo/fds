&HEAD CHID='divergence_test_3', TITLE='Simple 2D Tunnel'/ 

&MULT ID='mesh',DX=0.075,I_UPPER=3 /
&MESH IJK=75,1,40, XB=0.0,0.075,-0.001,0.001,-0.02,0.020, MULT_ID='mesh' /

&TIME T_END=2.0 /

&MISC SIMULATION_MODE='DNS', STRATIFICATION=.FALSE. /

&PRES TUNNEL_PRECONDITIONER=T /

&SPEC ID='LJ AIR', VISCOSITY=1.2E-5, BACKGROUND=.TRUE. /

&SURF ID='WALL', FREE_SLIP=.TRUE., DEFAULT=.TRUE. /
&SURF ID='PLATE', NO_SLIP=.TRUE. /

&DEVC ID='clock_1', XYZ=0,0,0, QUANTITY='TIME', SETPOINT=0.5, INITIAL_STATE=F /
&DEVC ID='clock_2', XYZ=0,0,0, QUANTITY='TIME', SETPOINT=1.5, INITIAL_STATE=T /
&OBST XB=0.100,0.102,-0.001,0.001,-0.005,0.005, SURF_ID='PLATE', DEVC_ID='clock_1' /
&OBST XB=0.210,0.212,-0.001,0.001,-0.005,0.005, SURF_ID='PLATE', DEVC_ID='clock_2' /

&SURF ID='BLOW', VEL=-0.5 /

&VENT PBX=0.0, SURF_ID='BLOW' /
&VENT PBX=0.3, SURF_ID='OPEN' /

&SLCF PBY=0.,QUANTITY='PRESSURE', VECTOR=T /
&SLCF PBY=0.,QUANTITY='DIVERGENCE', CELL_CENTERED=T /

&DEVC ID='div_max', QUANTITY='DIVERGENCE', XB=0.0,0.3,-0.001,0.001,-0.02,0.02, SPATIAL_STATISTIC='MAX' /
&DEVC ID='div_min', QUANTITY='DIVERGENCE', XB=0.0,0.3,-0.001,0.001,-0.02,0.02, SPATIAL_STATISTIC='MIN' /

&TAIL /


