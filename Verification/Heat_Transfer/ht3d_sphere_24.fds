&HEAD CHID='ht3d_sphere_24', TITLE='Heat transfer in solid sphere' /

&MESH IJK=24,24,24, XB=-0.12,0.12,-0.12,0.12,-0.12,0.12 /

&TIME T_END=180., DT=2. /

&MISC SOLID_PHASE_ONLY=T /

&RADI RADIATION=F /

&MATL ID            = 'STUFF'
      SPECIFIC_HEAT = 1
      CONDUCTIVITY  = 1
      DENSITY       = 1000 /

&SURF ID='SPHERE'
      MATL_ID='STUFF'
      HT3D=T
      INTERNAL_HEAT_SOURCE=200.
      HEAT_TRANSFER_COEFFICIENT=100000
      HEAT_TRANSFER_COEFFICIENT_BACK=100000
      CELL_SIZE=0.01
      COLOR='ORANGE' /

&MULT ID='cube array', DX=0.01, DY=0.01, DZ=0.01, I_UPPER=21, J_UPPER=21, K_UPPER=21 /

&OBST XB=-0.110,-0.100,-0.110,-0.100,-0.110,-0.100, MULT_ID='cube array', SHAPE='SPHERE', RADIUS=0.1, SURF_ID='SPHERE' /

&VENT MB='XMIN',SURF_ID='OPEN' /
&VENT MB='XMAX',SURF_ID='OPEN' /
&VENT MB='YMIN',SURF_ID='OPEN' /
&VENT MB='YMAX',SURF_ID='OPEN' /
&VENT MB='ZMIN',SURF_ID='OPEN' /
&VENT MB='ZMAX',SURF_ID='OPEN' /

&DUMP DT_PROF=10 /

&PROF XYZ=0.098,0,0, IOR=1, QUANTITY='TEMPERATURE', ID='TS' /

&TAIL /
